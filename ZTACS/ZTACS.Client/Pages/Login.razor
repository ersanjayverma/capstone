@page "/"
@using MudBlazor

<MudPaper Class="pa-6 mx-auto my-12" Elevation="4">
    <MudText Typo="Typo.h5" Color="Color.Primary">ZTACS Login</MudText>

    <MudTextField Label="Email" @bind-Value="email" Variant="Variant.Outlined" />
    <MudTextField Label="Password" @bind-Value="password" Variant="Variant.Outlined" InputType="InputType.Password" />
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="@Login1">Login</MudButton>
</MudPaper>

